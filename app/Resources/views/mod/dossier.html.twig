{% extends 'base.html.twig' %}

{% block body %}
    <div class="pageTitle">
        {% trans %}Modifier le dossier{% endtrans %}
    </div>
    
    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_end(form) }}
                 
{% endblock %}

{% block javascripts %}
    <script>
        $(document).ready(function() {
            $('#appbundle_dossier_nom').keyup(function() {
                this.value = this.value.toUpperCase();
            });
        });
    </script>
{% endblock %} 
    